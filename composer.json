{
    "name": "education/cwp-core",
    "description": "Common Web Platform base for all Education.govt.nz websites",
    "type": "silverstripe-vendormodule",
    "autoload": {
        "psr-4": {
            "Education\\Cwp\\": "src/",
            "Education\\Cwp\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3",
        "mockery/mockery": "dev-master@dev",
        "cwp/cwp-recipe-search": "^2.2",
        "silverstripe/contentreview": "^4.1",
        "silverstripe/spellcheck": "^2.1",
        "silverstripe/userforms": "^5.0",
        "silverstripe/iframe": "2.0.x-dev",
        "silverstripe/tagfield": "2.1",
        "silverstripe/sqlite3": "^2",
        "silverstripe/gridfieldqueuedexport": "2.x-dev",
        "education/google-analytics": "2.x-dev",
        "education/tasker": "dev-master",
        "education/tinymce-config": "dev-master",
        "education/login-screen": "2.x-dev",
        "silverstripe/sharedraftcontent": "^2.0@dev",
        "wilr/silverstripe-googlesitemaps": "2.x-dev",
        "undefinedoffset/silverstripe-nocaptcha": "2.0.x-dev",
        "jonom/silverstripe-betternavigator": "dev-master",
        "unclecheese/display-logic": "2.0.1",
        "silverstripe-terraformers/embargo-expiry": "^1.0@RC"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        },
        "expose": [
            "client"
        ]
    },
    "scripts": {
        "lint": "phpcs src/ tests/",
        "lint-clean": "phpcbf src/ tests/"
    },
    "repositories": [{
        "type": "composer",
        "url": "https://packages.educationweb.services"
    }]
}
